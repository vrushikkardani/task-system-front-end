<div id="admin_edit_project" class="modal custom-modal fade" role="dialog">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Edit Project Details</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="form-group">
                        <label>Task <span class="text-danger">*</span></label>
                        <input class="form-control" id="edittask" type="text" required>
                    </div>
                    <div class="form-group">
                        <label>Description <span class="text-danger">*</span></label>
                        <textarea rows="4" class="form-control summernote" id="editdescription"
                            placeholder="Enter your message here"></textarea>

                    </div>
                    <label class="focus-label">Allocate Employee Email Id</label>
                    <div class="form-group form-focus select-focus">
                        <select class="select floating" id="editemployeeid">

                        </select>

                    </div>
                    <div class="form-group">
                        <label>Start Time </label>
                        <input class="form-control" id="editstarttime" type="text" required>
                    </div>
                    <div class="form-group">
                        <label>End Time </label>
                        <input class="form-control" id="editendtime" type="text" required>
                    </div>
                    <div class="form-group">
                        <label>Date<span class="text-danger">*</span></label>
                        <input class="form-control datetimepicker" id="editdate" type="text">
                    </div>
                    <label class="focus-label">Status</label>
                    <div class="form-group form-focus select-focus">
                        <select class="select floating" id="editstatus">
                            <option value="To Do"> To Do </option>
                            <option value="On Going"> On Going </option>
                            <option value="Complete"> Complete </option>
                        </select>

                    </div>
                    <input class="form-control" id="id" type="text" hidden>

                    <p id="error1" class="text-center text-danger"></p>
                    <p id="ps" class="text-center text-danger"></p>
                    <div class="submit-section">
                        <button class="btn btn-primary submit-btn" onclick="update()">Submit</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>