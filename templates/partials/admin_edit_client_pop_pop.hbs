<div id="admin_edit_client" class="modal custom-modal fade" role="dialog">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Edit User Details</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form>
                   
                    <div class="form-group">
                        <label>Client Name <span class="text-danger">*</span></label>
                        <input class="form-control" id="editclientname" type="text">
                    </div>
                    <div class="form-group">
                        <label>Email Id <span class="text-danger">*</span></label>
                        <input class="form-control" id="editclientemail" type="text">
                    </div>
                    <div class="form-group">
                        <label>Contact Number <span class="text-danger">*</span></label>
                        <input class="form-control" id="editclientnumber" type="text" >
                    </div>
                    <div class="form-group">
                        <label>Address <span class="text-danger">*</span></label>
                        <textarea class="form-control" cols="4" id="editclientaddress"></textarea>
                    </div>
                    <div class="form-group">
                        <label>City <span class="text-danger">*</span></label>
                        <input class="form-control" id="editclientcity" type="text" >
                    </div>
                    
                    <p id="error1" class="text-center text-danger"></p>
                    <div class="form-group">
                        <input class="form-control" id="editclientid" type="text" hidden>
                    </div>
                    <p id="ps" class="text-center text-danger"></p>
                    <div class="submit-section">
                        <button class="btn btn-primary submit-btn" onclick="updateclient()">Submit</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
